{
  "name": "neimheadh/sonata-annotation-bundle",
  "type": "symfony-bundle",
  "description": "Symfony Bundle that adds annotations for Sonata Admin.",
  "keywords": [
    "symfony",
    "sonata",
    "sonata-admin",
    "annotations"
  ],
  "homepage": "https://github.com/kunicmarko20/SonataAnnotationBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Marko Kunic",
      "email": "kunicmarko20@gmail.com",
      "homepage": "https://twitter.com/kunicmarko20"
    },
    {
      "name": "Mathieu Wambre",
      "email": "contact@neimheadh.fr"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "doctrine/annotations": "^1.7",
    "sonata-project/admin-bundle": "^4.0",
    "symfony/config": "^5.4 || ^6.0",
    "symfony/dependency-injection": "^5.4 || ^6.0",
    "symfony/finder": "^5.4 || ^6.0",
    "symfony/http-kernel": "^5.4 || ^6.0"
  },
  "conflict": {
    "sonata-project/admin-bundle": "<4.0"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.0",
    "friends-of-behat/mink-browserkit-driver": "^1.6",
    "friends-of-behat/mink-extension": "^2.7",
    "friends-of-behat/symfony-extension": "^2.4",
    "matthiasnoback/symfony-dependency-injection-test": "^4.2",
    "phpstan/phpstan-phpunit": "^1.0",
    "sonata-project/doctrine-orm-admin-bundle": "^3.6 || ^4.0",
    "symfony/framework-bundle": "^5.4 || ^6.0",
    "symfony/phpunit-bridge": "^6.1",
    "symfony/var-dumper": "^5.0 || ^6.0"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev",
      "dev-develop": "4.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "KunicMarko\\SonataAnnotationBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KunicMarko\\SonataAnnotationBundle\\Tests\\": "tests/",
      "KunicMarko\\SonataAnnotationBundle\\Features\\Fixtures\\Project\\": "features/Fixtures/Project/src",
      "KunicMarko\\SonataAnnotationBundle\\Features\\": "features/"
    }
  }
}
